    "html-webpack-plugin": "^2.30.1",
    "localforage": "^1.5.0",
    "material-ui": "^1.0.0-beta.17",
    "material-ui-icons": "^1.0.0-beta.17",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-infinite-calendar": "^2.3.1",
    "react-motion": "^0.5.1",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-persist": "^4.9.1"
  },
  "devDependencies": {
